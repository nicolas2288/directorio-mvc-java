public class Producto {
    private String nombre;
    private String sku;
    private int cantidad;
    private double precio;

    public Pombre = nombre;
        this.sku = sku;
        this.cantidad = cantidad;
        this.precio = precio;
    }

    @Override
    public String toString() {
